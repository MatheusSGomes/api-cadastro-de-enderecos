                "operationId": "91b057fad8e8461b12343a98512f2b9a",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "codigo_uf": {
                                        "type": "integer"
                                    },
                                    "nome": {
                                        "type": "string"
                                    },
                                    "status": {
                                        "type": "integer"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "codigo_uf": "1",
                                    "nome": "TAGUATINGA",
                                    "status": 1
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Município cadastrado com sucesso."
                    },
                    "503": {
                        "description": "Não foi possível cadastrar o Município."
                    }
                }
            }
        },
